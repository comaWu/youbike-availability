{
  "builds": [
    { "src": "api/**/*.py", "use": "@vercel/python" },
    { "src": "web/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ],
  "buildCommand": "npm --prefix web run build",
  "outputDirectory": "web/dist"
}